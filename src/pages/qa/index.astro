---
import BaseLayout from '../../layouts/BaseLayout.astro';
import siteConfig from '../../data/site-config';
import PageHeader from '../../components/PageHeader.astro';
import QuestionsAndAnswers from '../../components/QuestionsAndAnswers.astro';

const { title, list } = siteConfig.qa;
---

<BaseLayout description={siteConfig.description} image={siteConfig.image} showHeader={false}>
  <PageHeader title={title} nbsp={false} />
  <QuestionsAndAnswers list={list} />
</BaseLayout>
